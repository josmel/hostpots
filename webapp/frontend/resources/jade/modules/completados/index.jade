	script(type="text/template").row4Table
		tr(class!="<%= css_class %>" ,data-id!="<%= id %>")
			td(data-title="Cód")
				div 
					a(href!="detalle-completados/<%= id %>") <%= codigo %>
			td(data-title="Tipo")
				div 
					a(href!="detalle-completados/<%= id %>") <%= tipo %>
			td(data-title="Destino")
				div 
					a(href!="detalle-completados/<%= id %>") <%= destino %>
			td(data-title="Contacto")
				div 
					a(href!="detalle-completados/<%= id %>") <%= contacto %> 
			td(data-title="Estado")
				div 
					a(href!="detalle-completados/<%= id %>") <%= estadoDesc %>
			td(data-title="TimeStamp")
				div 
					a(href!="detalle-completados/<%= id %>") <%= timeStamp %>
			td(data-title="Fecha")
				div 
					a(href!="detalle-completados/<%= id %>") <%= date %>
			td(data-title="Recibo")
				div
					a(href!="#")
						img(src!="{{asset('client/img/recibo.png')}}", alt="")
			td(data-title="Tarifa") 
				div 
					a(href!="detalle-completados/<%= id %>") <%= tarifa %>
